{{!--
{
  choiceGroup: Ember.A(), // Array of repoInfo objects
  toggleRepository: an Ember action
}
--}}
<ul class="list-group">
  {{#each choiceGroup as |repoInfo|}}
    <RepositoryCard
      @repository={{repoInfo.repository}}
      @funders={{repoInfo.funders}}
      @choice="true"
      @type="choice"
      @toggleRepository={{action "toggleRepository"}} />
  {{/each}}
</ul>
{{yield}}